    import java.util.*;
    public class Main
    {
    public static void main(String[] args)
    {
    Scanner in=new Scanner(System.in);
    int t=in.nextInt();
    String str[]=new String[t];
    for (int i = 0; i <t; i++)
    {
    str[i]="UnluckyChef";
    }
    for (int i = 0; i < t; i++)
    {
    int arr[]=new int[5];
    for (int j = 0; j < 4; j++)
    {
    arr[j]=in.nextInt();
    }
    arr[4]=arr[0]-arr[1];
    for (int j = 0; j <arr[3]; j++)
    {
    int arr2[]=new int[2];
    for (int k = 0; k < 2; k++)
    {
    arr2[k]=in.nextInt();
    }
    if(arr[2]>=arr2[1] && arr2[0]>=arr[4])
    str[i]="LuckyChef";
    }
    }
    for (int i = 0; i <t; i++)
    {
    System.out.println(str[i]);
    }
    }
    }
